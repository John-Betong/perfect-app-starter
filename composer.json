{
  "name": "krubio/perfect-app-skeleton",
  "description": "Perfect App Skeleton",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Kevin Rubio",
      "email": "krubio@galaxyinternet.us"
    }
  ],
  "repositories": [
      {
        "type": "package",
        "package": {
          "name": "krubio/perfect-app-framework",
          "version": "3.0",
          "source": {
            "url": "https://github.com/benanamen/perfect-app-framework.git",
            "type": "git",
            "reference": "origin/master"
          }
        }
      }
    ],
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "PerfectApp\\": "vendor/krubio/perfect-app-framework/src/"

    }
  },
  "require": {
    "php": ">=7.4",
    "ext-pdo": "*",
    "ext-ctype": "*",
    "ext-openssl": "*",
    "krubio/perfect-app-framework": "3.0",
    "ext-json": "*"
  },
  "require-dev": {
    "nunomaduro/phpinsights": "^2.0"
  }
}
